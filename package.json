{
  "name": "ionic-swipe-all",
  "version": "1.0.0",
  "description": "Allow Ionic (≥ v2) to recognize both horizontal and vertical swipe gesture.",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && ngc && npm run copy",
    "clean": "rimraf ./dist",
    "copy": "copyfiles ./src/* ./dist/",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diogoqueiros/ionic-swipe-all"
  },
  "author": {
    "name": "Diogo Queirós",
    "email": "diogoqueiros@outlook.com"
  },
  "keywords": [
    "angular",
    "ionic"
  ],
  "bugs": {
    "url": "https://github.com/diogoqueiros/ionic-swipe-all/issues"
  },
  "dependencies": {
    "@angular/common": "4.0.1",
    "@angular/compiler": "4.0.1",
    "@angular/compiler-cli": "4.0.1",
    "@angular/core": "4.0.1",
    "@angular/platform-browser": "4.0.1",
    "@angular/platform-browser-dynamic": "4.0.1",
    "hammerjs": "2.0.8"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@types/hammerjs": "2.0.34",
    "codelyzer": "^2.0.0-beta.1",
    "copyfiles": "1.2.0",
    "rimraf": "^2.5.4",
    "tslint": "^3.15.1",
    "typescript": "2.2.2"
  },
  "engines": {
    "node": ">=0.8.0"
  }
}
