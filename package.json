{
  "name": "ionic-swipe-all",
  "version": "1.1.0",
  "description": "Allow Ionic (≥ v2) to recognize both horizontal and vertical swipe gesture.",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && ngc && npm run clean-src",
    "clean": "rimraf ./dist",
    "clean-src": "rimraf ./src/*.ngsummary.json & rimraf ./src/*.ngfactory.ts",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diogoqueiros/ionic-swipe-all"
  },
  "author": {
    "name": "Diogo Queirós",
    "email": "diogoqueiros@outlook.com"
  },
  "keywords": [
    "angular",
    "ionic"
  ],
  "bugs": {
    "url": "https://github.com/diogoqueiros/ionic-swipe-all/issues"
  },
  "dependencies": {
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/compiler-cli": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "hammerjs": "2.0.8"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@types/hammerjs": "2.0.34",
    "rimraf": "^2.5.4",
    "typescript": "2.4.2"
  },
  "engines": {
    "node": ">=0.8.0"
  }
}
